{
  "name": "BrowseIt, for Knowit",
  "short_name": "BrowseIt",
  "version": "3.0.1",
  "manifest_version": 2,
  "offline_enabled": true,

  "description": "For the noobs and the veterans alike. Has all important links, your introductory todolist, and a map of meeting rooms.",
  "homepage_url": "https://knowit.no",
  "permissions": ["https://projects.knowit.no/*"],

  "browser_action": {
    "default_title": "BrowseIt",
    "default_popup": "index.html"
  },
  "icons": {
    "48": "/48.png",
    "128": "/128.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
